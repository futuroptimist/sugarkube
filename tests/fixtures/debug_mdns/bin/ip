#!/bin/bash
subcommand="$1"
shift || true
case "$subcommand" in
  addr)
    cat <<'OUT'
2: eth0: <BROADCAST,MULTICAST,UP,LOWER_UP> mtu 1500
    link/ether aa:bb:cc:dd:ee:ff brd ff:ff:ff:ff:ff:ff
    inet 192.168.10.5/24 brd 192.168.10.255 scope global eth0
    inet6 fe80::aabb:ccff:fedd:eeff/64 scope link
OUT
    ;;
  route)
    echo "224.0.0.0/4 dev eth0 proto kernel scope link src 10.0.0.5"
    ;;
  maddress)
    echo "2: eth0"
    echo "    link 33:33:00:00:00:fb"
    ;;
  *)
    echo "ip stub invoked"
    ;;
esac
